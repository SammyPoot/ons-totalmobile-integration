    <soapenv:Header/>
    <soapenv:Body>
        <mes:TransformAndSendRequest>
            <mes:Targets>
                <!--Zero or more repetitions:-->
                <mes:Target>
                    <mes:QueueName>?</mes:QueueName>
                    <mes:Format>?</mes:Format>
                </mes:Target>
            </mes:Targets>
            <mes:Format>?</mes:Format>
            <mes:Content>?</mes:Content>
            <!--Optional:-->
            <mes:Key>?</mes:Key>
            <mes:XPath>?</mes:XPath>
        </mes:TransformAndSendRequest>
    </soapenv:Body>
